import org.sireum._

//return the smallest element in list
def min(list: ZS): Z = {

  var small: Z = list(0)
  var i: Z = 1
  
  while (i < list.size) {

    if (list(i) < small) {
      small = list(i)
    }
    i = i + 1
  }

  return small
}

